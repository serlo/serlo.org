{
  "name": "@serlo/editor-renderer",
  "version": "0.1.2",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm-run-all build:private build:babel build:docker",
    "build:private": "(cd ../../../; yarn build:private)",
    "build:babel": "cross-env NODE_ENV=production babel src --extensions \".ts,.tsx\" --out-dir dist --root-mode upward --source-maps",
    "build:docker": "ts-node scripts/build",
    "predeploy": "yarn build",
    "deploy": "ts-node scripts/deploy",
    "prestart": "yarn build:private",
    "start": "nodemon --watch src --exec \"ts-node src/index.ts\""
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@serlo/editor": "^0.5.0",
    "@serlo/editor-helpers": "^0.0.0",
    "@serlo/legacy-editor-to-editor": "^0.0.0",
    "babel-runtime": "^6.0.0",
    "body-parser": "^1.0.0",
    "express": "^4.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "styled-components": "^4.0.0"
  },
  "devDependencies": {
    "@serlo/docker": "^0.0.0",
    "@types/express": "^4.0.0",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^16.0.0",
    "@types/styled-components": "^4.0.0",
    "nodemon": "^1.0.0"
  },
  "private": true
}
